using System;
 namespace quiz
{
    class Program
    {
        static void Main(string[] args)
        {
            int Num1;

            Console.Write("Enter the number: ");
            Num1 = Convert.ToInt32(Console.ReadLine());

            if (Num1 >=0)
            {
                Console.Write("The given number is POSITIVE");
                Console.ReadLine();
            }
            else
            {
                Console.Write("The given number is NEGATIVE");
                Console.ReadLine();
            }
            Console.ReadKey(true);
        }
    }
}
